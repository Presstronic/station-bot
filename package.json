{
    "name": "station-bot",
    "version": "0.0.1",
    "description": "A Discord bot for verifying users against their roberts space industries profiles.",
    "main": "./dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "typeorm": "tsx ./node_modules/typeorm/cli.js",
        "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/station-bot-initial-migration -d ./src/data-source.ts",
        "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts"        
    },
    "type": "module",
    "author": "Demian",
    "license": "GPL-3.0",
    "dependencies": {
        "@nestjs/typeorm": "^10.0.2",
        "axios": "^1.7.8",
        "cheerio": "^1.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "discord.js": "^14.11.0",
        "dotenv": "^16.3.1",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.20",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/axios": "^0.9.36",
        "@types/node": "^20.17.10",
        "@types/winston": "^2.4.4",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsx": "^4.19.2",
        "typescript": "^5.1.6"
    }
}
